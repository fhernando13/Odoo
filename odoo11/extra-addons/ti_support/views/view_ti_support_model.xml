<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <data>

    <!-- model.name form view -->
    <record id="ti_support_model_view_form" model="ir.ui.view">
      <field name="name">ti_support.ti_support_model.view.form</field>
      <field name="model">ti_support.ti_support_model</field>
      <field name="arch" type="xml">
        <form string="Modelos">
          <sheet>
            <group>
              <!-- Add your fields here -->
              <field name="name"/>
              <field name="name_id"/>
              <field name="anio"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>


    <!-- model.name tree view -->
    <record id="ti_support_model_view_tree" model="ir.ui.view">
      <field name="name">ti_support.ti_support_model.view.tree</field>
      <field name="model">ti_support.ti_support_model</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="name_id"/>
          <field name="anio"/>
        </tree>
      </field>
    </record>


    <record model="ir.actions.act_window" id="ti_support.model_action_window">
        <field name="name">Modelos</field>
        <field name="res_model">ti_support.ti_support_model</field>
        <field name="view_mode">tree,form</field>
    </record>


  </data>
</odoo>